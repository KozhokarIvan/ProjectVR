{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    },
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "Seq",
          "Args": {
            "serverUrl": "http://projectvr-seq:80"
          }
        }
      ],
      "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
      "Properties": {
        "Application": "ProjectVR"
      }
    },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ProjectVR": "YourConnectionString"
  }
}
